{
    "presets": [
        [
            "es2015",
            {
                "loose": true
            }
        ],
        "react"
    ],
    "plugins": [
        "react-hot-loader/babel",
        "transform-class-properties",
        "transform-object-rest-spread",
        [
            "import",
            {
                "libraryName": "antd",
                "libraryDirectory": "lib",
                "style": "css"
            }
        ] 
        
    ]
}